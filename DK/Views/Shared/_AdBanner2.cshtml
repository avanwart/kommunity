@using BootBaronLib.AppSpec.DasKlub.BOL.DomainConnection
@using BootBaronLib.Operational
@using BootBaronLib.Values
<!-- 160 x 600 -->
@{
    int randomID = Utilities.RandomNumber(1, 4);

    switch (randomID)
    {
        case 1:
            @Html.Raw(SiteDomain.GetSiteDomainValue(
                SiteEnums.SiteBrandType.A1601,
                Utilities.GetCurrentLanguageCode()))
            break;
        case 2:
            @Html.Raw(SiteDomain.GetSiteDomainValue(
                SiteEnums.SiteBrandType.A1602,
                Utilities.GetCurrentLanguageCode()))
            break;
        case 3:

            @Html.Raw(SiteDomain.GetSiteDomainValue(
                SiteEnums.SiteBrandType.A1603,
                Utilities.GetCurrentLanguageCode()))

            break;
        default:

            @Html.Raw(SiteDomain.GetSiteDomainValue(
                SiteEnums.SiteBrandType.A1603,
                Utilities.GetCurrentLanguageCode()))
            break;
    }
}